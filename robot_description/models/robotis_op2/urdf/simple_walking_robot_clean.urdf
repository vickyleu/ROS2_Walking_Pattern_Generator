<?xml version="1.0" ?>
<!-- 简洁外观版本 - 使用基础几何体 -->
<robot name="simple_walking_robot_clean">

  <!-- 基础 -->
  <link name="base_link"/>
  
  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="body_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- 躯干 - 用Box替代有洞的mesh -->
  <link name="body_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.05 0.12"/>
      </geometry>
      <material name="DarkGrey">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.05 0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.975"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- ==================== 左腿 ==================== -->
  <joint name="l_hip_pitch" type="revolute">
    <parent link="body_link"/>
    <child link="l_thigh_link"/>
    <origin xyz="0 0.037 -0.06"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="1.57" velocity="5.65"/>
  </joint>

  <link name="l_thigh_link">
    <visual>
      <origin xyz="0 0 -0.0465" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.018" length="0.093"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.5 0.5 0.55 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.0465"/>
      <geometry>
        <cylinder radius="0.018" length="0.093"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.0465"/>
      <mass value="0.119"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="l_knee" type="revolute">
    <parent link="l_thigh_link"/>
    <child link="l_shin_link"/>
    <origin xyz="0 0 -0.093"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="0.0" velocity="5.65"/>
  </joint>

  <link name="l_shin_link">
    <visual>
      <origin xyz="0 0 -0.0465" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.015" length="0.093"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.5 0.5 0.55 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.0465"/>
      <geometry>
        <cylinder radius="0.015" length="0.093"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.0465"/>
      <mass value="0.0703"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="l_ankle_pitch" type="revolute">
    <parent link="l_shin_link"/>
    <child link="l_foot_link"/>
    <origin xyz="0 0 -0.093"/>
    <axis xyz="0 1 0"/>
    <limit effort="2.8" lower="-0.78" upper="0.78" velocity="5.65"/>
  </joint>

  <link name="l_foot_link">
    <visual>
      <origin xyz="0.02 0 -0.01" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.04 0.02"/>
      </geometry>
      <material name="DarkGrey">
        <color rgba="0.35 0.35 0.4 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.02 0 -0.01"/>
      <geometry>
        <box size="0.08 0.04 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.02 0 -0.01"/>
      <mass value="0.0794"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- ==================== 右腿 ==================== -->
  <joint name="r_hip_pitch" type="revolute">
    <parent link="body_link"/>
    <child link="r_thigh_link"/>
    <origin xyz="0 -0.037 -0.06"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="1.57" velocity="5.65"/>
  </joint>

  <link name="r_thigh_link">
    <visual>
      <origin xyz="0 0 -0.0465" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.018" length="0.093"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.5 0.5 0.55 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.0465"/>
      <geometry>
        <cylinder radius="0.018" length="0.093"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.0465"/>
      <mass value="0.119"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="r_knee" type="revolute">
    <parent link="r_thigh_link"/>
    <child link="r_shin_link"/>
    <origin xyz="0 0 -0.093"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="0.0" velocity="5.65"/>
  </joint>

  <link name="r_shin_link">
    <visual>
      <origin xyz="0 0 -0.0465" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.015" length="0.093"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.5 0.5 0.55 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.0465"/>
      <geometry>
        <cylinder radius="0.015" length="0.093"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.0465"/>
      <mass value="0.0703"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="r_ankle_pitch" type="revolute">
    <parent link="r_shin_link"/>
    <child link="r_foot_link"/>
    <origin xyz="0 0 -0.093"/>
    <axis xyz="0 1 0"/>
    <limit effort="2.8" lower="-0.78" upper="0.78" velocity="5.65"/>
  </joint>

  <link name="r_foot_link">
    <visual>
      <origin xyz="0.02 0 -0.01" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.04 0.02"/>
      </geometry>
      <material name="DarkGrey">
        <color rgba="0.35 0.35 0.4 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.02 0 -0.01"/>
      <geometry>
        <box size="0.08 0.04 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.02 0 -0.01"/>
      <mass value="0.0794"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

</robot>

