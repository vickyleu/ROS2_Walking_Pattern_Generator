<?xml version="1.0" ?>
<!-- 美化版机器人 - 流线型设计 -->
<robot name="simple_walking_robot_clean">

  <link name="base_link"/>
  
  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="body_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- 躯干 - 圆润的主体 -->
  <link name="body_link">
    <!-- 主躯干 -->
    <visual>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <geometry>
        <box size="0.07 0.045 0.10"/>
      </geometry>
      <material name="BodyMain">
        <color rgba="0.15 0.15 0.2 1.0"/>
      </material>
    </visual>
    <!-- 胸部装饰 -->
    <visual>
      <origin xyz="0.025 0 0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.025 0.035 0.06"/>
      </geometry>
      <material name="Accent">
        <color rgba="0.2 0.5 0.8 1.0"/>
      </material>
    </visual>
    <!-- 腰部 -->
    <visual>
      <origin xyz="0 0 -0.04" rpy="0 0 0"/>
      <geometry>
        <box size="0.065 0.05 0.025"/>
      </geometry>
      <material name="BodyDark">
        <color rgba="0.1 0.1 0.12 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.07 0.05 0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.975"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- ==================== 左腿 ==================== -->
  <joint name="l_hip_pitch" type="revolute">
    <parent link="body_link"/>
    <child link="l_thigh_link"/>
    <origin xyz="0 0.024 -0.055"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="1.57" velocity="5.65"/>
  </joint>

  <link name="l_thigh_link">
    <!-- 髋关节球 -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.018"/>
      </geometry>
      <material name="Joint">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <!-- 大腿主体 -->
    <visual>
      <origin xyz="0 0 -0.045" rpy="0 0 0"/>
      <geometry>
        <capsule radius="0.014" length="0.065"/>
      </geometry>
      <material name="Limb">
        <color rgba="0.25 0.25 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.045"/>
      <geometry>
        <cylinder radius="0.016" length="0.09"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.045"/>
      <mass value="0.119"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="l_knee" type="revolute">
    <parent link="l_thigh_link"/>
    <child link="l_shin_link"/>
    <origin xyz="0 0 -0.09"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="0.0" velocity="5.65"/>
  </joint>

  <link name="l_shin_link">
    <!-- 膝关节球 -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.016"/>
      </geometry>
      <material name="Joint">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <!-- 小腿主体 -->
    <visual>
      <origin xyz="0 0 -0.045" rpy="0 0 0"/>
      <geometry>
        <capsule radius="0.012" length="0.065"/>
      </geometry>
      <material name="Limb">
        <color rgba="0.25 0.25 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.045"/>
      <geometry>
        <cylinder radius="0.014" length="0.09"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.045"/>
      <mass value="0.0703"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="l_ankle_pitch" type="revolute">
    <parent link="l_shin_link"/>
    <child link="l_foot_link"/>
    <origin xyz="0 0 -0.09"/>
    <axis xyz="0 1 0"/>
    <limit effort="2.8" lower="-0.78" upper="0.78" velocity="5.65"/>
  </joint>

  <link name="l_foot_link">
    <!-- 踝关节球 -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.012"/>
      </geometry>
      <material name="Joint">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <!-- 脚掌 -->
    <visual>
      <origin xyz="0.018 0 -0.008" rpy="0 0 0"/>
      <geometry>
        <box size="0.07 0.035 0.012"/>
      </geometry>
      <material name="Foot">
        <color rgba="0.2 0.2 0.25 1.0"/>
      </material>
    </visual>
    <!-- 脚尖 -->
    <visual>
      <origin xyz="0.048 0 -0.008" rpy="0 0 0"/>
      <geometry>
        <box size="0.015 0.03 0.012"/>
      </geometry>
      <material name="Accent">
        <color rgba="0.2 0.5 0.8 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.02 0 -0.008"/>
      <geometry>
        <box size="0.08 0.04 0.015"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.02 0 -0.008"/>
      <mass value="0.0794"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <!-- ==================== 右腿 ==================== -->
  <joint name="r_hip_pitch" type="revolute">
    <parent link="body_link"/>
    <child link="r_thigh_link"/>
    <origin xyz="0 -0.024 -0.055"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="1.57" velocity="5.65"/>
  </joint>

  <link name="r_thigh_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.018"/>
      </geometry>
      <material name="Joint">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <visual>
      <origin xyz="0 0 -0.045" rpy="0 0 0"/>
      <geometry>
        <capsule radius="0.014" length="0.065"/>
      </geometry>
      <material name="Limb">
        <color rgba="0.25 0.25 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.045"/>
      <geometry>
        <cylinder radius="0.016" length="0.09"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.045"/>
      <mass value="0.119"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="r_knee" type="revolute">
    <parent link="r_thigh_link"/>
    <child link="r_shin_link"/>
    <origin xyz="0 0 -0.09"/>
    <axis xyz="0 -1 0"/>
    <limit effort="2.8" lower="-1.57" upper="0.0" velocity="5.65"/>
  </joint>

  <link name="r_shin_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.016"/>
      </geometry>
      <material name="Joint">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <visual>
      <origin xyz="0 0 -0.045" rpy="0 0 0"/>
      <geometry>
        <capsule radius="0.012" length="0.065"/>
      </geometry>
      <material name="Limb">
        <color rgba="0.25 0.25 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.045"/>
      <geometry>
        <cylinder radius="0.014" length="0.09"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.045"/>
      <mass value="0.0703"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="r_ankle_pitch" type="revolute">
    <parent link="r_shin_link"/>
    <child link="r_foot_link"/>
    <origin xyz="0 0 -0.09"/>
    <axis xyz="0 1 0"/>
    <limit effort="2.8" lower="-0.78" upper="0.78" velocity="5.65"/>
  </joint>

  <link name="r_foot_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.012"/>
      </geometry>
      <material name="Joint">
        <color rgba="0.3 0.3 0.35 1.0"/>
      </material>
    </visual>
    <visual>
      <origin xyz="0.018 0 -0.008" rpy="0 0 0"/>
      <geometry>
        <box size="0.07 0.035 0.012"/>
      </geometry>
      <material name="Foot">
        <color rgba="0.2 0.2 0.25 1.0"/>
      </material>
    </visual>
    <visual>
      <origin xyz="0.048 0 -0.008" rpy="0 0 0"/>
      <geometry>
        <box size="0.015 0.03 0.012"/>
      </geometry>
      <material name="Accent">
        <color rgba="0.2 0.5 0.8 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.02 0 -0.008"/>
      <geometry>
        <box size="0.08 0.04 0.015"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.02 0 -0.008"/>
      <mass value="0.0794"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

</robot>
